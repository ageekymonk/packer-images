{
  "variables": {
    "aws_access_key": "",
    "aws_secret_key": "",
    "aws_default_region": "",
    "instance_type": "t2.micro",
    "ami_prefix": "rhel-base-7.4",
    "ami_version": ""
  },
  "builders":
  [
    {
      "type": "amazon-ebs",
      "access_key": "{{user `aws_access_key`}}",
      "secret_key": "{{user `aws_secret_key`}}",
      "region": "{{user `aws_default_region`}}",
      "source_ami_filter": {
        "filters": {
          "virtualization-type": "hvm",
          "name": "RHEL-7.4_HVM_GA*",
          "root-device-type": "ebs"
        },
        "most_recent": true
      },
      "instance_type": "t2.micro",
      "ssh_username": "ubuntu",
      "ami_name": "{{user `ami_prefix`}}-{{user `ami_version`}}"
    }
  ],
  "provisioners":
  [
  ],
  "post-processors":
  [
  ]

}
